@model List<car_rental_Unicom.tic.Vew_modal.home_view_modal>

<h1>Your Bookings</h1>

<table class="table">
    <thead>
        <tr>
            <th>Booking ID</th>
            <th>Car ID</th>
            <th>Start Date</th>
            <th>Days</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var booking in Model)
        {
            <tr>
                <td>@booking.booking_id</td>
                <td>@booking.car_id</td>
                <td>@booking.start_date</td>
                <td>@booking.dayes</td>
                <td>@booking.boking_status</td>
            </tr>
        }
    </tbody>
</table>